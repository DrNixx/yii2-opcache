{
	"name": "insolita/yii2-opcache",
	"description": "Yii2 OpCache module - Show statistic, config, reset all, invalidate files, search in cached files",
	"type": "yii2-extension",
	"keywords": ["yii2","extension","opcache"],
	"license": "MIT",
	"authors": [
		{
			"name": "insolita",
			"email": "webmaster100500@ya.ru"
		}
	],
	"require": {
		"php":">=5.6.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap5": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"\\insolita\\opcache\\": "src/"
		}
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
